{
  "name": "opdocai",
  "version": "0.0.0",
  "private": true,
  "description": "OpDoc AI",
  "license": "UNLICENSED",
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:ng": "ng serve",
    "ng": "ng",
    "start": "ng serve",
    "build": "npm-run-all build:*",
    "build:ng": "ng build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ng": "ng build --watch --configuration development",
    "test": "npm run watch:test",
    "test:coverage": "ng test --coverage",
    "watch:test": "ng test --watch",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@typescript-eslint/parser": "8.27.0",
    "eslint": "9.22.0",
    "@angular-builders/jest": "19.0.0",
    "@angular/build": "19.2.4",
    "@angular/cli": "19.2.4",
    "@angular/compiler-cli": "19.2.3",
    "@types/node": "22.13.10",
    "@types/jest": "29.5.14",
    "angular-eslint": "19.2.1",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.2.6",
    "jest-preset-angular": "14.5.3",
    "jest-sonar-reporter": "2.0.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0",
    "npm-run-all2": "7.0.2",
    "@prettier/plugin-xml": "3.4.1",
    "prettier": "3.5.3",
    "prettier-plugin-gherkin": "3.1.2",
    "prettier-plugin-java": "2.6.7",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.10",
    "husky": "9.1.7",
    "lint-staged": "15.5.0"
  },
  "dependencies": {
    "@angular/animations": "19.2.3",
    "@angular/cdk": "19.2.5",
    "@angular/common": "19.2.3",
    "@angular/compiler": "19.2.3",
    "@angular/core": "19.2.3",
    "@angular/material": "19.2.5",
    "@angular/forms": "19.2.3",
    "@angular/platform-browser": "19.2.3",
    "@angular/platform-browser-dynamic": "19.2.3",
    "@angular/router": "19.2.3",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  }
}
